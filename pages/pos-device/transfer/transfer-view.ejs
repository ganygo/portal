        serviceName:{name:'posDevice.service.name',             caption:'Servis adÄ±',  type:'label'}, 
        localConnectorName:{name:'posDevice.localConnector.name',      caption:'Local Connector',  type:'label'}, 
        status:{name:'status',                            caption:'Durum',     type:'label'},
