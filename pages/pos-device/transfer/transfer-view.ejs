<div class="row">
    <div class="col-lg-6 col-md-6">
        
    <% if(typeof form!='undefined') { %>
        <a id="fileDownload" href="" download="ornek_data.json"><i class="fa fa-file"></i> ornek_data.json</a>
    <% } %>
    </div>
</div> 
<script type="text/javascript">
